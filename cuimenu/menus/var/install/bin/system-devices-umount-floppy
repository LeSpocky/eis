#!/bin/sh
#----------------------------------------------------------------------------
# system-devices-umount-floppy - umount floppy disk
#
# Creation   : 19.07.2003 fm  Frank Meyer <frank@eisfair.org>
# Last Update: $Id: system-devices-format-floppy 24471 2012-11-14 21:14:01Z jv $
#
#----------------------------------------------------------------------------

umount /media/floppy
if [ $? = 0 ]
then
    /var/install/bin/ask.cui --info "Floppy umount successful."
else
    /var/install/bin/ask.cui --error "Floppy umount failed!"
fi

exit 0
