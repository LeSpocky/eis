  <builders>
    <hudson.tasks.Shell>
      <command>if [ -r eis/.git ] ; then
    cd eis
    git pull
    cd ..
else
    git clone ssh://git@nettworks-git:7999/EIS/eis.git
fi
mv eisfair-ng.buildsettings.txt eis/_ADMIN/settings.txt

cd eis/_ADMIN

targetDir=`pwd`/jobTemplates
echo $targetDir
cd ../../../../
for currentTemplate in `ls _eisfair-ng__*/config.xml` ; do
    echo $currentTemplate
    if [ ! -d $targetDir/${currentTemplate%/*} ] ; then
        mkdir -p $targetDir/${currentTemplate%/*} 
    fi
    cp $currentTemplate $targetDir/${currentTemplate}
done
cd $WORKSPACE/eis
git add _ADMIN/jobTemplates/*
if git commit -m &quot;Updated administrative job configurations&quot; ; then
    git push
fi
</command>
    </hudson.tasks.Shell>
