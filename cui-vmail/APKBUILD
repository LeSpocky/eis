pkgname=cui-vmail
pkgver=1.0.1
pkgrel=0
pkgdesc="CUI setup for postfix/dovecot with virtual mail user"
url="http://www.eisfair.org"
arch="noarch"
license="GPL"
depends="cuimenu postfix postfix-mysql postfix-pcre pfqueue smc-milter-new dovecot dovecot-mysql dovecot-sample-config dovecot-pigeonhole mysql2sieve mysql2fetchmail fetchmail ca-certificates greyfix"
depends_dev=""
makedepends="$depends_dev"
install="$pkgname.pre-upgrade $pkgname.post-upgrade $pkgname.post-install $pkgname.pre-deinstall $pkgname.post-deinstall"
subpackages=""
source="${pkgname}.default
		${pkgname}.check
		${pkgname}.check.exp
		${pkgname}.help
		${pkgname}.init
		${pkgname}.postfixchroot
		${pkgname}.sh
		${pkgname}-update.sh
		${pkgname}.menu
		${pkgname}.modules.menu
		${pkgname}.tools.menu
		${pkgname}.user.menu
		${pkgname}.bogusmx.cidr
		${pkgname}.dynblocks.pcre
		${pkgname}.mimeblocks.pcre
		${pkgname}-mysql-canonical_maps.cf
		${pkgname}-mysql-client_access.cf
		${pkgname}-mysql-client_access_postscreen.cf
