&nbsp;&nbsp; mail\.spamserver\.tld<br/>
&nbsp;&nbsp; host[0-9]{1,3}\..*\.org<br/>
&nbsp;&nbsp; (.*[0-9]{1,3}[-.][0-9]{1,3}[-.][0-9]{1,3}.*\..*\..*)<br/>
