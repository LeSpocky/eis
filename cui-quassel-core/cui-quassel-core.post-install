#!/bin/sh

cd /var/install/bin
ln -s check-service.sh quasselcore.start
ln -s check-service.sh quasselcore.stop
ln -s check-service.sh quasselcore.status
ln -s check-service.sh quasselcore.restart
cd - > /dev/null

/var/install/bin/add-menu setup.services.menu setup.services.cui-quassel-core.menu "Quasselcore administration"

/var/install/config.d/cui-quassel-core-update.sh

exit 0
