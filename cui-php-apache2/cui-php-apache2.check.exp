PHP_MYSQL_SOCKET   = '/run/mysqld/mysqld.sock|()'
                    : 'Use only /run/mysqld/mysqld.sock or blank'
PHP_MYSQL_HOST     = 'localhost|(RE:FQDN)|(RE:IPADDR)|()'
                    : 'Use only: localhost, hostname.domain.tld, ip-address or empty for not use'
PHP_CACHING_MODULE = 'no|apc|xcache'
                    : 'Use "apc" for the APC chaching module or "xcache" for XCache. default = "no" -> no chaching modul'
